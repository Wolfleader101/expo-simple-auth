{
  "name": "expo-simple-auth",
  "version": "0.0.0",
  "description": "An easy to use, OAuth 2.0 and OpenID Connect (OIDC) authentication library built on top of Expo",
  "keywords": [
    "react",
    "react-native",
    "expo",
    "auth",
    "authentication",
    "oauth",
    "openid",
    "connect",
    "oidc",
    "jwt"
  ],
  "homepage": "https://github.com/Wolfleader101/expo-simple-auth",
  "repository": {
    "type": "git",
    "url": "https://github.com/Wolfleader101/expo-simple-auth.git"
  },
  "author": "Wolfleader101",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "dev": "tsup --watch",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "build": "tsup",
    "release": "changeset",
    "ci:publish": "pnpm build && changeset publish --access public"
  },
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitest/eslint-plugin": "^1.1.36",
    "eslint": "^9.21.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "prettier": "^3.5.3",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.7"
  }
}
